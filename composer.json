{
    "name": "undjike/plan-subscription-system",
    "description": "Plans, features and subscriptions management system for your applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Ulrich Pascal Ndjike Zoa",
            "email": "ndjikezoaulrich@gmail.com"
        }
    ],
    "require": {
        "ext-pdo": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.22",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Undjike\\PlanSubscriptionSystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Undjike\\PlanSubscriptionSystem\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Undjike\\PlanSubscriptionSystem\\PlanSubscriptionServiceProvider"
            ]
        }
    }
}
